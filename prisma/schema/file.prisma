model File {
  id       Int    @id @default(autoincrement())
  name     String
  original String
  mimeType String
  size     Int
  path     String
  bucket   String

  companyId    Int?
  uploadedById Int?
  uploadedBy   User? @relation(fields: [uploadedById], references: [id])

  createdAt DateTime @default(now())
}
