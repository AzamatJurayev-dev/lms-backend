enum TestStatus {
  DRAFT
  GENERATED
  PUBLISHED
  ARCHIVED
}

model Test {
  id          Int           @id @default(autoincrement())
  title       String
  duration    Int
  status      TestStatus    @default(DRAFT)
  questions   Question[]
  createdById Int
  createdBy   User          @relation(fields: [createdById], references: [id])
  createdAt   DateTime      @default(now())
  attempts    TestAttempt[]
}
